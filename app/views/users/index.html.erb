 <div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">

       <%= render'/books/info', user: @user, book: @book %>
    </div>

    <h2>Users</h2>
    <table>
        <thead>
            <tr>
                <th>image</th>
                <th>name</th>
            </tr>
        </thead>
        <tbody>
            <% @users.each do |u| %>
            <tr>
                <td><%=u.image_tag get_profile_image(100,100) %></td>
                <td><%= u.name %></td>
                <td><%= link_to'show', user_path.find(params[:id] %></td>
            </tr>
            <% end %>
        </tbody>
    </table>


  </div>
 </div>


